{
  "ConnectionStrings": {
    "DbConnection": "Data Source=../../Data/Database.sqlite"
  },
  "KeyVaultConfiguration": {
    "Uri": " https://keyvault.vault.azure.net/",
    "Enabled": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "AspNetCore.HealthChecks": "Error",
      "Elastic.Apm": "Error"
    }
  },
  "AllowedHosts": "*",
  "FileStorage": {
    "FilePath": "ClientApp/build/Images",
    "RequestPath": "/Images"
  },
  "DataProtection": {
    "enabled": false,
    "redisConnection": "redis",
    "redisKey": "AzureDevOps_Web"
  },
  "HealthChecksUI": {
    "Webhooks": [],
    "EvaluationTimeinSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "MemoryHealthConfiguration": {
    "Degraded": 10,
    "Unhealthy": 20
  },
  "ApiHealthConfiguration": {
    "Enabled": false,
    "Endpoints": [
      {
        "Name": "APM Http",
        "Uri": "http://apm-server:8200",
        "Tags": [
          "Monitoring",
          "Port:8200"
        ],
        "FailureStatus": "Degraded",
        "HttpMethod": "GET"
      },
      {
        "Name": "ElasticSearch Http",
        "Uri": "http://es01:9200",
        "Tags": [
          "Infrastructure",
          "Logging",
          "Port:9200"
        ],
        "FailureStatus": "Unhealthy",
        "HttpMethod": "GET"
      },
      {
        "Name": "Kibana Http",
        "Uri": "http://kib01:5601/status",
        "Tags": [
          "Metrics",
          "Port 5601"
        ],
        "FailureStatus": "Degraded",
        "HttpMethod": "Head"
      },
      {
        "Name": "Traefik Http",
        "Uri": "http://traefik:8080/ping",
        "Tags": [
          "Infrastructure",
          "Port:6379"
        ],
        "FailureStatus": "Degraded",
        "HttpMethod": "Get"
      }
    ]
  },
  "ElasticSearchConfiguration": {
    "ElasticUrl": "http://es01:9200",
    "Enabled": false
  },
  "Endpoints": {
    "AppConfig": "https://devopskats-config.azconfig.io"
  }
}